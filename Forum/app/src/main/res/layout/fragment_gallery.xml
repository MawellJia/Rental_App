<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.gallery.GalleryFragment">

    <Spinner
        android:id="@+id/suburb"
        android:layout_width="58dp"
        android:layout_height="29dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="150dp"
        app:layout_constraintStart_toEndOf="@+id/street"
        app:layout_constraintTop_toBottomOf="@+id/text5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:text="Bathroom"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/bathroom"
        app:layout_constraintTop_toBottomOf="@+id/text5" />

    <Spinner
        android:id="@+id/bathroom"
        android:layout_width="59dp"
        android:layout_height="28dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/text5" />

    <Spinner
        android:id="@+id/province"
        android:layout_width="58dp"
        android:layout_height="29dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="193dp"
        android:autofillHints="Province"
        app:layout_constraintStart_toEndOf="@+id/suburb"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/bedroom"
        android:layout_width="58dp"
        android:layout_height="29dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="83dp"
        android:autofillHints="Province"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/street"
        android:layout_width="58dp"
        android:layout_height="29dp"
        android:layout_marginTop="81dp"
        app:layout_constraintTop_toBottomOf="@+id/bathroom"
        tools:layout_editor_absoluteX="156dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="101dp"
        android:layout_height="28dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="40dp"
        android:text="Bedroom"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/bedroom"
        app:layout_constraintTop_toBottomOf="@+id/text5" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="37dp"
        android:text="Address:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bedroom" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Add Description"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="378dp" />

    <TextView
        android:id="@+id/text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="76dp"
        android:text="Add Description"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text3" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="182dp"
        android:layout_height="27dp"
        android:text="rent  out your house"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/unit"
        android:layout_width="70dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/number_input_bg"
        android:hint="Unit(Optional)"
        android:inputType="number"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="256dp" />

    <EditText
        android:id="@+id/Buil_no"
        android:layout_width="63dp"
        android:layout_height="26dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/number_input_bg"
        android:hint="Buliding_No"
        android:inputType="number"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/unit"
        tools:layout_editor_absoluteY="256dp" />

</androidx.constraintlayout.widget.ConstraintLayout>