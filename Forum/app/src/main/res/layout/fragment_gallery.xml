<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.gallery.GalleryFragment">

    <Spinner
        android:id="@+id/bedroom"
        android:layout_width="58dp"
        android:layout_height="29dp"
        android:autofillHints="Province"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="65dp" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address:"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="131dp" />

    <Spinner
        android:id="@+id/suburb"
        android:layout_width="58dp"
        android:layout_height="29dp"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="175dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="65dp" />

    <EditText
        android:id="@+id/numberEditText2"
        android:layout_width="63dp"
        android:layout_height="26dp"
        android:background="@drawable/number_input_bg"
        android:hint="Buliding_No"
        android:inputType="number"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="178dp" />

    <Spinner
        android:id="@+id/province"
        android:layout_width="58dp"
        android:layout_height="29dp"
        android:autofillHints="Province"
        tools:layout_editor_absoluteX="329dp"
        tools:layout_editor_absoluteY="175dp" />

    <Spinner
        android:id="@+id/bathroom"
        android:layout_width="58dp"
        android:layout_height="29dp"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="65dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="65dp" />

    <EditText
        android:id="@+id/numberEditText"
        android:layout_width="70dp"
        android:layout_height="26dp"
        android:background="@drawable/number_input_bg"
        android:hint="Unit(Optional)"
        android:inputType="number"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="178dp" />

    <Spinner
        android:id="@+id/street"
        android:layout_width="58dp"
        android:layout_height="29dp"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="175dp" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Description"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="221dp" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="rent out your house"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Picture Description"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="352dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="upload"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="434dp" />

</androidx.constraintlayout.widget.ConstraintLayout>